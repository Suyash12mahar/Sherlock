<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.android.example.sherlock">
    <uses-permission android:name="android.permission.INTERNET"/>
    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".DisplaySeason" />
        <activity android:name=".Season1Activity" />
        <activity android:name=".EpisodeItem" />
        <activity android:name=".TestActivity" />
        <activity android:name=".EpisodeDetails" />
        <activity android:name=".DisplayEpisodes" />
        <activity android:name=".ViewVideoTest"></activity>
    </application>

</manifest>